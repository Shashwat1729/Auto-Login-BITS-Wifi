{
  "manifest_version": 3,
  "name": "Auto Login Extension",
  "version": "1.2",
  "description": "Automatically logs in to specified portals.",
  "icons": {
    "16": "icons/icon128.png",
    "48": "icons/icon128.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs", 
    "scripting", 
    "storage", 
    "alarms"
  ],
  "host_permissions": [
    "http://172.16.0.30:8090/",
    "http://172.16.100.117/"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icons/icon128.png"
    }
  },
  "options_page": "options.html"
}
